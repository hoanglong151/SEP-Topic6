@model IEnumerable<SEPQuestionAnswer.Models.Question>

@{   
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}


<section id="category" class="contact">
    <table class="table">
        <tr>
            <th>
                Câu hỏi
            </th>          
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <a href="@Url.Action("Details", "Questions", new { id = item.ID})">@item.AskQuestion</a>
                </td>
                <td>
                    Views: @item.CountView
                </td>
            </tr>
        }

    </table>
</section>